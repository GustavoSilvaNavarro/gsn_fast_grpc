{
  "name": "shn_gsn_tree",
  "version": "1.0.0",
  "description": "Tree service api.",
  "main": "src/index.ts",
  "scripts": {
    "build": "docker build .",
    "start": "cross-env SEED_TABLE='true' ts-node src/index.ts | ./node_modules/.bin/bunyan",
    "start:prod": "npm run start",
    "dev": "set -a && source .env && nodemon src/index.ts | ./node_modules/.bin/bunyan",
    "clear-cache": "jest --clear-cache",
    "test": "jest -c jest.config.ts",
    "test:int": "jest -c jest.integration.config.ts -i",
    "test:int:local": "set -a && source .env && npm run test:int",
    "wait:integration": "./wait-for.sh http://api:8080/healthz npm run integration",
    "integration": "set -a && source testing.env && npm run test:int",
    "lint": "npx eslint src/ tests/",
    "lint:fix": "npx eslint src/ tests/ --fix",
    "format": "npx prettier --write src tests",
    "check": "npx prettier --check src tests",
    "prepare": "husky install",
    "migration:up": "npx node-pg-migrate up",
    "migration:down": "npx node-pg-migrate down",
    "proto:gen": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/proto-gen --ts_proto_opt=esModuleInterop=true,outputServices=grpc-js --proto_path=./src/protos --proto_path=./node_modules/google-proto-files ./src/protos/*.proto"
  },
  "author": "Gustavo Silva",
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "@prisma/client": "^6.15.0",
    "axios": "^1.11.0",
    "bunyan": "^2.0.5",
    "google-proto-files": "^5.0.1",
    "signal-exit": "^4.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.48.4",
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@tsconfig/recommended": "^1.0.10",
    "@types/bunyan": "^1.8.11",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/signal-exit": "^3.0.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.1.2",
    "jest-extended": "^6.0.0",
    "jest-when": "^3.7.0",
    "nodemon": "^3.1.9",
    "npq": "^3.11.2",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.1",
    "ts-proto": "^2.7.7",
    "typescript-transform-paths": "^3.5.5"
  }
}
